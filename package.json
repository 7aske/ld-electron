{
	"name": "ld",
	"version": "1.0.0",
	"description": "",
	"main": "./dist/main/app.js",
	"scripts": {
		"start": "electron ./dist/main/app.js",
		"webpack": "npx webpack --config webpack.config.js",
		"package-linux": "electron-packager . deployment-client-electron --overwrite --prune=true --platform=linux --arch=x64 --out=packaged/linux --ignore=\"./src\"",
		"package-win": "electron-packager . deployment-client-electron --overwrite --prune=true --platform=win32 --arch=x64 --out=packaged/windows --ignore=\"./src\"",
		"package-pi": "electron-packager . deployment-client-electron --overwrite --prune=true --platform=linux --arch=armv7l --out=packaged/raspi --ignore=\"./src\"",
		"package-mac": "electron-packager . deployment-client-electron --overwrite --prune=true --platform=darwin --arch=x64 --out=packaged/mac --ignore=\"./src\""
	},
	"author": "Nikola Tasic",
	"license": "ISC",
	"devDependencies": {
		"electron": "^2.0.6",
		"ts-loader": "^4.5.0",
		"webpack": "^4.17.1",
		"webpack-cli": "^3.1.0"
	},
	"dependencies": {
		"@babel/core": "^7.0.0",
		"@types/axios": "^0.14.0",
		"@types/shortid": "0.0.29",
		"axios": "^0.18.0",
		"bootstrap": "^4.1.3",
		"shortid": "^2.2.13"
	}
}
